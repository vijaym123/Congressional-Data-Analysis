  "introduced_at": "2011-04-15", 
  "number": "1653", 
  "official_title": "To amend the Tariff Act of 1930 to increase the dollar amount requirements for articles and merchandise under the administrative exemptions and entry under regulations provisions of that Act.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": "18", 
    "name": "Schock, Aaron", 
    "state": "IL", 
    "thomas_id": "01920", 
    "title": "Rep", 
    "type": "person"
  }, 
  "status": "REFERRED", 
  "status_at": "2011-04-15", 
  "subjects": [
    "Foreign trade and international finance", 
    "Tariffs"
  ], 
  "subjects_top_term": "Foreign trade and international finance", 
  "summary": {
    "as": "Introduced", 
    "date": "2011-04-15", 
    "text": "Amends the Tariff Act of 1930 to increase from $200 to $1,000 the aggregate retail value of articles that may be imported duty-free into the United States by one person on one day. Changes from a maximum of $2,500 to a minimum of $2,500 the threshold aggregate value of a shipment of merchandise for the declaration and entry of which the Secretary of the Treasury is authorized to prescribe rules and regulations."
  }, 
