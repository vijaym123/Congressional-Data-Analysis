  "short_title": "Pool Safety and Accessibility for Everyone (Pool SAFE) Act", 
  "sponsor": {
    "district": "5", 
    "name": "Mulvaney, Mick", 
    "state": "SC", 
    "thomas_id": "02059", 
    "title": "Rep", 
    "type": "person"
