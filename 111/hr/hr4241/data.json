  "introduced_at": "2009-12-08", 
  "number": "4241", 
  "official_title": "To amend chapter 17 of title 38, United States Code, to allow for increased flexibility in payments for State veterans homes.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": "2", 
