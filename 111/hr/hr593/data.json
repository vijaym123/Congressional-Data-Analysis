  "introduced_at": "2009-01-15", 
  "number": "593", 
  "official_title": "To amend title 10, United States Code, to expand the authorized concurrent receipt of disability severance pay from the Department of Defense and compensation for the same disability under any law administered by the Department of Veterans Affairs to cover all veterans who have a combat-related disability, as defined under section 1413a of such title.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": "9", 
    "name": "Smith, Adam", 
